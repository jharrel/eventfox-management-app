<h1>
<%= @project.name %>
</h1>

<%= link_to "Edit Project", edit_project_path(@project), class: "btn btn-sm btn-info btn-primary" %>  

<%= link_to "Delete Project", @project, method: :delete, class: "btn btn-sm btn-danger btn-primary" %> 





<% tasks = @project.tasks %>
<br><br>
<%= render partial: 'tasks/tasklist', :locals => {:@tasks => tasks}%>
